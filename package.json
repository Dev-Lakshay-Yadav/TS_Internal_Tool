{
  "name": "ts_internal_tool",
  "version": "1.0.0",
  "description": "Full-stack TypeScript application with React and Express",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "npm run start --workspace=server",
    "start:client": "npm run start --workspace=client",
    "build": "npm run build --workspaces",
    "dev": "concurrently -n SERVER,CLIENT -c bgBlue.bold,bgGreen.bold \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/form-data": "^2.2.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.15.31",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react-redux": "^7.1.34",
    "@types/three": "^0.178.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "dotenv-webpack": "^8.1.1",
    "typescript": "^5.4.5"
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.10.0",
    "box-node-sdk": "^3.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "form-data": "^4.0.3",
    "three": "^0.178.0",
    "three-stdlib": "^2.36.0",
    "use-debounce": "^10.0.6"
  }
}
